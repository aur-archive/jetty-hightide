# set JAVA_HOME
[ -f /etc/profile.d/jre.sh ] && . /etc/profile.d/jre.sh
[ -f /etc/profile.d/jdk.sh ] && . /etc/profile.d/jdk.sh
[ -f /etc/profile.d/openjdk6.sh ] && . /etc/profile.d/openjdk6.sh
# or set your variant
#JAVA_HOME=/somewhere
JAVA_OPTIONS=

JETTY_USER=jetty
JETTY_HOME=/opt/jetty
# Comment out next line if need
JETTY_CONF=$JETTY_HOME/jetty.conf
JETTY_PORT=8080
JETTY_RUN=/run/jetty
JETTY_PID=/run/jetty.pid
# Jetty's variables
#JETTY_ARGS=
